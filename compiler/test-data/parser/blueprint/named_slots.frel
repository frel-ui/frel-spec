module test

blueprint Article(header: Blueprint, content: Blueprint, footer: Blueprint) {
    column {
        header()
        content()
        footer()
    }
}

blueprint Page {
    Article {
        at header: { text { "Header" } }
        at content: {
            text { "Content line 1" }
            text { "Content line 2" }
        }
        at footer: { text { "Footer" } }
    }
}
