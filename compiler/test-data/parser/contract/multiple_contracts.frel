module contract.multiple_contracts

import test.common.User
import test.common.Product
import test.common.Item
import test.common.Order

contract UserAPI {
    get_user(id: Uuid) : User
    update_user(id: Uuid, data: User)
}

contract ProductAPI {
    get_product(id: Uuid) : Product
    list_products() : List<Product>
}

contract OrderAPI {
    create_order(userId: Uuid, items: List<Item>) : Order
    get_order(id: Uuid) : Order
    cancel_order(id: Uuid)
}
